+// syscall 320
+long getnicebypid(pid_t pid, int *retval) {
+	struct task_struct *p;
+	int ret;
+
+	p = find_task_by_vpid(pid);
+	if (!p) {
+		//printk(KERN_INFO "\033[31m Cannot get task with pid %d.\n \033[0m", pid);
+		ret = 65535;
+	}
+	else
+		ret = TASK_NICE(p);
+
+	if (copy_to_user(retval, &ret, sizeof(int)))
+		return -EFAULT;
+	return 1;
+}
+
+// syscall 321
+long setnicebycolor(u_int16_t color, int nice) {
+	struct task_struct *task,*p;
+	struct list_head *pos;
+	int count=0;
+	int prio;
+
+	if (color < 0 || color > MAX_COLOR_NR) {
+		//printk(KERN_INFO "\033[31m The color is invalid %d.\n \033[0m", color);
+		return -1;
+	}
+
+	if (nice < -20 || nice > 19) {
+		//printk(KERN_INFO "\033[31m The nice value is invalid %d.\n \033[0m", nice);
+		return -2;
+	}
+
+	nice = format_nice(nice);
+
+
+	prio = NICE_TO_PRIO(nice);
+	//printk("\033[32m Set nice(%d) by color(%d).\n \033[0m", nice, color);
+	task=&init_task;
+	list_for_each(pos,&task->tasks)
+	{
+		p=list_entry(pos,struct task_struct,tasks);
+		count++;
+		if (p->color == color) {
+			p->static_prio = prio;
+			//printk("\033[32m Prio of proc pid=%d is set to %d.\n \033[0m", p->pid, prio);
+		}
+	}
+
+	return count;
+}
+
+
+int lab2_init_outer_list(void) {
+	/*
+	 Check if the outer list is created.
+	 Use the variable lab2_init_list to as a flag;
+	 */
+
+	struct task_struct *task,*p;
+	struct list_head *pos;
+	int count;
+
+	if (!lab2_init_list) {
+		INIT_LIST_HEAD(&outer);
+		++lab2_init_list;
+
+		/*
+		 Add all the current task to the list.
+		 */
+
+		task = &init_task;
+		list_for_each(pos, &task->tasks)
+		{
+			p = list_entry(pos, struct task_struct, tasks);
+			count++;
+			lab2_add_task(p);
+		}
+
+		//printk("\033[34m Init outer list. [%d] task added.\n \033[0m", count);
+		return 0;
+
+
+		/*
+		 * Init cpu subsystem for all colors.
+		 */
+	}
+	return -1;
+}
+
+int lab2_add_color(struct task_struct *p) {
+	u_int16_t color = p->color;
+	struct color_node *c_node;
+	c_node = (struct color_node *)kmalloc(sizeof(struct color_node), GFP_KERNEL);
+	INIT_LIST_HEAD(&(c_node->inner)); // init inner list
+	c_node->color = p->color; // set color value
+	c_node->quota = -1;
+	c_node->nice = PRIO_TO_NICE(p->static_prio); // set color <-> nice pair
+	list_add(&(c_node->list), &outer); // add color to outer loop
+	return 0;
+}
+
+
+int lab2_add_task(struct task_struct *p) {
+	/*
+	 Add a task to list when a new process is created.
+	 First to check if the outer list is created.
+	 Then add a new task to the list;
+	 */
+
+	// add a new task
+	u_int16_t target_color;
+	struct list_head *cpos;
+	struct color_node *cn;
+	int nice;
+	struct task_node *tn;
+
+	/*
+	 Check if the outer list is created.
+	 If the outer list is not created, then add all tasks to tht list
+	 and return 0.
+	 */
+	if( !(lab2_init_outer_list()) )
+		return 0;
+
+
+	target_color = p->color;
+
+	tn = (struct task_node *)kmalloc(sizeof(struct task_node), GFP_KERNEL);
+	tn->p = p;
+
+	list_for_each(cpos, &outer) {
+		cn = list_entry(cpos, struct color_node, list);
+		if (cn->color == target_color) {
+			nice = cn->nice;
+			p->static_prio = NICE_TO_PRIO(nice);
+			list_add(&(tn->list), &(cn->inner));
+			//printk("\033[33m Add task [%d] to inner list.\n \033[0m", p->pid);
+			return 1;
+		}
+	}
+
+	//printk("\033[33m Add color [%d] to outer list.\n \033[0m", target_color);
+	lab2_add_color(p);
+	return lab2_add_task(p);
+}
+
+int lab2_remove_task(struct task_struct *p) {
+	struct list_head *cpos;
+	struct list_head *tpos;
+	struct list_head *m; // temp pointer for list_for_each_safe
+	struct list_head *n; // temp pointer for list_for_each_safe
+	struct color_node *cn;
+	struct task_node *tn;
+	int cnt;
+
+	list_for_each_safe(cpos, m, &outer) {
+		cn = list_entry(cpos, struct color_node, list);
+		if (p->color == cn->color){
+			list_for_each_safe(tpos, n, &(cn->inner)) {
+				tn = list_entry(tpos, struct task_node, list);
+				if (tn->p == p) {
+					list_del(tpos);
+					kfree(tn);
+					//printk("\033[36m Task [%d] is removed from lab2 list.\n \033[0m", p->pid);
+					cnt = 0;
+					list_for_each(tpos, &(cn->inner)) {
+						++cnt;
+					}
+					//printk("\033[36m %d tasks in color[%d] now.\n \033[0m", cnt, cn->color);
+					break;
+				}
+			}
+		}
+	}
+	return 1;
+}
