+static int A2I(char *buf) {
+	int ret = 0;
+	int ptr = 0;
+	if (buf[0] == '-' && buf[1] == '1')
+		return -1;
+	while (ptr<15 && buf[ptr]<='9' && buf[ptr]>='0'){
+		ret = 10*ret + (buf[ptr++]-'0');
+	}
+	return ret;
