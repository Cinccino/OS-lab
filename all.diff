+
+int format_nice(int nice) {
+
+	/* format nice */
+	if (nice <= -12)
+		return -20;
+	else if (nice > -12 && nice <= -4)
+		return -10;
+	else if (nice > -4 && nice <= 4)
+		return 0;
+	else if (nice > 4 && nice <= 12)
+		return 10;
+	else
+		return 19;
+}
