+int ext2_is_inode_free(struct super_block *sb, unsigned long ino) {
+	int error = -EINVAL;
+	unsigned long block_group, bit;
+	struct ext2_group_desc *desc;
+	struct buffer_head *bh = NULL;
+	if (ino < EXT2_FIRST_INO(sb) ||
+		ino > le32_to_cpu(EXT2_SB(sb)->s_es->s_inodes_count)) {
+		ev_dbg_error("[ext2_is_inode_free] ino = %lu is invalid.", ino);
+		return error;
+	}
+
+	block_group = (ino-1) / EXT2_INODES_PER_GROUP(sb);
+	bit = (ino-1) % EXT2_INODES_PER_GROUP(sb);
+	desc = ext2_get_group_desc(sb, block_group, NULL);
+
+	if (!desc) {
+		ev_dbg_error("[ext2_is_inode_free] Cannot find desc of inode {%lu}", ino);
+		goto error_out;
+	}
+
+	bh = sb_bread(sb, le32_to_cpu(desc->bg_inode_bitmap));
+	if (!bh) {
+		ev_dbg_error("[ext2_is_inode_free] Cannot get a buffer header.");
+		goto error_out;
+	}
+
+	/* Test the inode bitmap now. */
+	error = !test_bit(bit, (void *) bh->b_data);
+	ev_dbg_info("[ext2_is_inode_free] is_inode_free: %lu, %d", ino, error);
+
+error_out:
+	brelse(bh);
+	return error;
+}
