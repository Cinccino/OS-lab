+int clfs_open(struct inode *i_node, struct file *filp) {
+	int error;
+
+	/* Check if the file is evicted or not. */
+	if (!i_node->i_op->getxattr)
+		return -EOPNOTSUPP;
+
+	/* Lock inode to prevent concurrent eviction while doing
+	 * open/fetch
+	 */
+	ev_dbg_info("clfs_open: locking inode %lu", i_node->i_ino);
+	mutex_lock(&i_node->i_mutex);
+
+	ev_dbg_info("clfs_open: opening inode %lu", i_node->i_ino);
+
+	/* Fetch the file if it is evicted. */
+	error = ext2_is_evicted(i_node);
+	if (error > 0)
+		error = ext2_fetch(i_node);
+
+	mutex_unlock(&i_node->i_mutex);
+
+	if (error < 0)
+		return -EACCES;
+	return generic_file_open(i_node, filp);
+}
