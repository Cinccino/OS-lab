+//318
+int getcolors(int nr_pids, pid_t *pids, u_int16_t *colors, int *retval) {
+
+	pid_t *pids_kernel = (pid_t *)kmalloc(MAX_PID_NR*sizeof(pid_t), GFP_KERNEL);
+	u_int16_t *colors_kernel = (u_int16_t *)kmalloc(MAX_PID_NR*sizeof(u_int16_t), GFP_KERNEL);
+	int *retval_kernel = (int *)kmalloc(MAX_PID_NR*sizeof(int), GFP_KERNEL);
+	struct task_struct *p;
+	int i;
+	int ret = 0;
+
+//	printk(KERN_INFO "\033[31m A\n \033[0m");
+	if (copy_from_user(pids_kernel, pids, nr_pids*sizeof(pid_t)))
+		return -EFAULT;
+//	printk(KERN_INFO "\033[31m B\n \033[0m");
+	if (copy_from_user(colors_kernel, colors, nr_pids*sizeof(u_int16_t)))
+		return -EFAULT;
+//	printk(KERN_INFO "\033[31m C\n \033[0m");
+	if (copy_from_user(retval_kernel, retval, nr_pids*sizeof(int)))
+		return -EFAULT;
+
+//	printk(KERN_INFO "\033[31m In color.c getcolors(%d)\n \033[0m", nr_pids);
+
+	for (i = 0; i < nr_pids; i++) {
+		p = find_task_by_vpid(pids_kernel[i]);
+		if (!p){
+			retval_kernel[i] = EFAULT;
+		}
+		else {
+			retval_kernel[i] = 0;
+			colors_kernel[i] = p->color;
+			ret++;
+		}
+		printk(KERN_INFO "\033[30m getcolor pid=%d\tcolor=%d\n \033[0m", pids_kernel[i], colors_kernel[i]);
+	}
+
+	if (copy_to_user(retval, retval_kernel, nr_pids*sizeof(int)))
+		return -EFAULT;
+	if (copy_to_user(colors, colors_kernel, nr_pids*sizeof(u_int16_t)))
+		return -EFAULT;
+
+
+	return ret;
+}
+
+//319
+int setcolors(int nr_pids, pid_t *pids, u_int16_t *colors, int *retval) {
+
+	pid_t *pids_kernel = (pid_t *)kmalloc(MAX_PID_NR*sizeof(pid_t), GFP_KERNEL);
+	u_int16_t *colors_kernel = (u_int16_t *)kmalloc(MAX_PID_NR*sizeof(u_int16_t), GFP_KERNEL);
+	int *retval_kernel = (int *)kmalloc(MAX_PID_NR*sizeof(int), GFP_KERNEL);
+	struct task_struct *p;
+	int i;
+	int ret = 0;
+
+
+	printk(KERN_INFO "\033[31m A\n \033[0m");
+
+	if (current_uid())
+		return -EACCES;
+
+	printk(KERN_INFO "\033[31m B\n \033[0m");
+	if (copy_from_user(pids_kernel, pids, nr_pids*sizeof(pid_t)))
+		return -EFAULT;
+
+	printk(KERN_INFO "\033[31m C\n \033[0m");
+	if (copy_from_user(colors_kernel, colors, nr_pids*sizeof(u_int16_t)))
+		return -EFAULT;
+	printk(KERN_INFO "\033[31m D\n \033[0m");
+	if (copy_from_user(retval_kernel, retval, nr_pids*sizeof(int)))
+		return -EFAULT;
+
+	printk(KERN_INFO "\033[31m In color.c setcolors(%d)\n \033[0m", nr_pids);
+
+	for (i = 0; i < nr_pids; i++) {
+		p = find_task_by_vpid(pids_kernel[i]);
+		if (!p){
+			retval_kernel[i] = EFAULT;
+		}
+		else {
+			if (colors_kernel[i] >= MAX_COLOR_NR)
+				retval_kernel[i] = EFAULT;
+			else {
+				lab2_remove_task(p);
+				retval_kernel[i] = 0;
+				p->color = colors_kernel[i];
+				ret++;
+				lab2_add_task(p);
+			}
+		}
+		printk(KERN_INFO "\033[30m setcolor pid=%d\tcolor=%d\n \033[0m", pids_kernel[i], colors_kernel[i]);
+	}
+
+
+	if (copy_to_user(retval, retval_kernel, nr_pids*sizeof(int)))
+		return -EFAULT;
+
+	return ret;
+}
