+unsigned long ext2_next_inode(struct super_block *sb, unsigned long ino) {
+	unsigned long new_ino;
+	new_ino = (ino+1) % le32_to_cpu(EXT2_SB(sb)->s_es->s_inodes_count);
+	if (new_ino == 0)
+		new_ino = EXT2_FIRST_INO(sb);
+
+	BUG_ON(new_ino != ino + 1 && new_ino != EXT2_FIRST_INO(sb));
+
+	ev_dbg_info("[ext2_next_inode] next inode of [%lu] is %lu", ino, new_ino);
+	return new_ino;
+}
