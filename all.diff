--- /dev/null
+++ b/fs/evictd.c
@@ -0,0 +1,77 @@
+#include <linux/kthread.h>
+#include <linux/init.h>
+#include <linux/sched.h>
+#include <linux/fs.h>
+
+#define PERIOD_SEC 10
+
+#define EXT2_DEBUG 0
+
+#ifdef EXT2_DEBUG
+#define ev_dbg_error(msg...) do {		\
+	printk(KERN_INFO "\033[1;31m");		\
+	printk(KERN_INFO msg);				\
+	printk(KERN_INFO "\033[0m\n");		\
+} while(0)
+#define ev_dbg_info(msg...) do {		\
+	printk(KERN_INFO "\033[1;32m");		\
+	printk(KERN_INFO msg);				\
+	printk(KERN_INFO "\033[0m\n");		\
+} while(0)
+//char dbg[50];
+
+#else
+#define ev_dbg_error(msg...) do{} while(0)
+#define ev_dbg_info(msg...) do{} while(0)
+//char dbg[50];
+
+#endif
+
+/* The evictor thread. Invoke FS eviction
+ * once every 60 seconds.
+ */
+
+static int kevictd_thread(void *data) {
+	struct super_block *sb;
+	ev_dbg_info("[thread] kevictd: started kthread");
+
+	set_current_state(TASK_INTERRUPTIBLE);
+
+	while (!kthread_should_stop()) {
+		/* Wait for 60 seconds. */
+		set_current_state(TASK_INTERRUPTIBLE);
+		schedule_timeout(PERIOD_SEC * HZ);
+		ev_dbg_info("[thread] In evict loop.");
+		ev_dbg_info("[thread] Current pid = %d", current->pid);
+
+		spin_lock(&sb_lock);
+		list_for_each_entry(sb, &super_blocks, s_list) {
+			if (sb->s_op->evict_fs) {
+				/* Increment reference counter */
+				sb->s_count++;
+				spin_unlock(&sb_lock);
+				/* Prevent unmounting */
+				down_read(&sb->s_umount);
+				ev_dbg_info("[thread] In evict loop.");
+				sb->s_op->evict_fs(sb);
+				up_read(&sb->s_umount);
+				spin_lock(&sb_lock);
+				sb->s_count--;
+			}
+		}
+		spin_unlock(&sb_lock);
+	}
+	__set_current_state(TASK_RUNNING);
+	return 0;
+}
+
+
+static int __init kevictd_init(void) {
+	struct task_struct *p;
+	p = kthread_run(kevictd_thread, NULL, "kevictd");
+	if (IS_ERR(p))
+		return (int)p;
+
+	return 0;
+}
+fs_initcall(kevictd_init);
