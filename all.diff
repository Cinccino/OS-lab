+
+#define EXT2_DEBUG 0
+
+#ifdef EXT2_DEBUG
+#define ev_dbg_error(msg...) do {		\
+	printk(KERN_INFO "\033[1;31m");		\
+	printk(KERN_INFO msg);				\
+	printk(KERN_INFO "\033[0m\n");		\
+} while(0)
+#define ev_dbg_info(msg...) do {		\
+	printk(KERN_INFO "\033[1;32m");		\
