+/* Return 1 if the inode is opened by some other process.
+ * 0 if inode is safe to evict. Assumes that the caller
+ * has one reference count on the inode.
+ */
+static int is_inode_open(struct inode *i_node) {
+	struct dentry *d_entry;
+	ev_dbg_info("[is_inode_open] In function.");
+	/* i_count of 1 or less, then definitely unused. */
+	if (atomic_read(&i_node->i_count) <= 1)
+		return 0;
+
+
+	ev_dbg_info("[is_inode_open] Ready to loop \"list_for_each_entry\".");
+	hlist_for_each_entry(d_entry, &i_node->i_dentry, d_alias) {
+		if (atomic_read(&d_entry->d_count) > 0)
+			return 1;
+	}
+	return 0;
+
+}
