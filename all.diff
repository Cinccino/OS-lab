+#define ev_dbg_info(msg...) do{} while(0)
+//char dbg[50];
+
+#endif
+
+DEFINE_MUTEX(evict_mutex);
+
+/* Return the next used inode after ino. May return
+ * error. The wrap variable is incremented every time
+ * we wrap around the FS.
+ */
+
+static long ext2_next_used_inode(struct super_block *sb,
+							unsigned long ino, int *wrap) {
+	long error = -EINVAL;
+	unsigned long oldino = ino;
+	unsigned long group, bit;
+
+	// Block buffer
+	struct buffer_head *bh = NULL;
+	struct ext2_group_desc *desc;
+
+	// Get next inode number
+	if (ino < EXT2_FIRST_INO(sb) ||
+		ino >= le32_to_cpu(EXT2_SB(sb)->s_es->s_inodes_count) ) {
+		ino = EXT2_FIRST_INO(sb);
+	}
+	else {
+		ino = ino + 1;
+	}
+
+	group = (ino - 1) / EXT2_INODES_PER_GROUP(sb);
+	bit = (ino - 1) % EXT2_INODES_PER_GROUP(sb);
+
+	while (1) {
+		// Read bitmap
+		desc = ext2_get_group_desc(sb, group, NULL);
+		if (!desc) {
+			ev_dbg_error("[ext2_next_used_inode] Could not fetch descriptor of group [%lu]", group);
+			error = -EINVAL;
+			goto error_out;
+		}
+		brelse(bh);
+		bh = sb_bread(sb, le32_to_cpu(desc->bg_inode_bitmap));
+		if (!bh) {
+			error = -EINVAL;
+			ext2_error(sb, "read_inode_bitmap",
+				   "Cannot read inode bitmap - "
+				   "group = %lu, inode_bitmap = %u",
+				   group, le32_to_cpu(desc->bg_inode_bitmap));
+			goto error_out;
+		}
+
+		// Find next used bit
+		bit = ext2_find_next_bit((unsigned long *)bh->b_data,
+							EXT2_INODES_PER_GROUP(sb), bit);
+
+		// If bit is within current block, done.
+		if (bit < EXT2_INODES_PER_GROUP(sb))
+			break;
+
+		// Else, search from the next group.
+		bit = 0;
+		if (++group == EXT2_SB(sb)->s_groups_count) {
+			bit = EXT2_FIRST_INO(sb) - 1;
+			group = 0;
+			if (wrap)
+				(*wrap)++;
+		}
+	}
+
+	// The next inode
+	error = group*EXT2_INODES_PER_GROUP(sb)+bit+1;
+	ev_dbg_info("[ext2_next_used_inode] old_inode: %ld, next_inode: %ld", oldino, error);
+
+error_out:
+	brelse(bh);
+	return error;
+}
