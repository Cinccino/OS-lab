+static int clsrv_rw(enum rw_t read, struct ext2_sb_info *sbi,
+		    struct socket *sock, const void *buf,
+		    const size_t length)
+{
+	int len, done = 0, left = length;
+	struct kvec kv;
+	struct msghdr msg = {
+		.msg_name       = (struct sockaddr *)&sbi->cl_srv_addr,
+		.msg_namelen    = sizeof(sbi->cl_srv_addr),
+		.msg_control    = NULL,
+		.msg_controllen = 0,
+		.msg_flags      = MSG_WAITALL
+	};
+
+	do {
+		ev_dbg_info("clsrv_rw: send/recv message, read=%d, "
+			 "left=%d, done=%d",
+			 (read == OP_READ), left, done);
+		kv.iov_len = left;
+		kv.iov_base = (char *) buf + done;
+		len = (read == OP_READ) ?
+			kernel_recvmsg(sock, &msg, &kv, 1, left, 0) :
+			kernel_sendmsg(sock, &msg, &kv, 1, left);
+
+		ev_dbg_info("clsrv_rw: send/recv message done, len=%d", len);
+
+		if (len < 0) {
+			done = len;
+			break;
+		}
+		done += len;
+		left -= len;
+	} while (len && left);
+
+	return done;
+}
