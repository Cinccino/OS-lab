diff --git a/mm/ssmem.c b/mm/ssmem.c
new file mode 100644
index 0000000..a8cead6
--- /dev/null
+++ b/mm/ssmem.c
@@ -0,0 +1,244 @@
+/*
+ * linux/mm/ssmem.c
+ *
+ * COMS W4118, Columbia University, HW4 solution
+ *
+ * Instructor: Kaustubh Joshi
+ * Copyright (C) 2013 Kaustubh Joshi
+ */
+
+/*
+ * Three ways to do ssmem: a) Use a master task whose pgtable is the
+ * reference. Have to copy ptes if master dies.  b) Update everyone's
+ * ptes every time a new page is allocated. Identify other tasks
+ * through rmap (anon_vma).  c) Use a seperate per shmem data
+ * structure containing pointers to struct page entries. Easiest.
+ *
+ * Our solution uses (c) because it is easiest and can be done without
+ * reverse mapping (both the others need a reverse map to identify the
+ * AS that maps this ssmem segment). Unfortunately, this solution
+ * doesn't work out of the box with swapping, because the struct page,
+ * may not be stable when swapping is enabled. To support swapping, we
+ * need to replace ssmem page map with swap_pte when page is to be
+ * swapped in vmscan.c.
+ */
+
+#include <linux/syscalls.h>
+#include <linux/mm_types.h>
+#include <linux/list.h>
+
+#include <linux/mm.h>
+#include <linux/rmap.h>
+#include <linux/mutex.h>
+#include <linux/swap.h>
+#include <linux/mman.h>
+#include <linux/mempolicy.h>
+#include <asm/tlb.h>
+
+#include "internal.h"
+
+#define MAX_SSMEM 1024
+#define MAX_PAGES 16
+
+#define SSMEM_CREATE 0x1
+#define SSMEM_WRITE  0x2
+#define SSMEM_EXEC   0x4
+
+typedef struct mutex mutex_t;
+
+struct ssmem_t {
+	mutex_t lock;                /* Lock protecting alloced_pages */
+	int valid;
+	int nr_mappers;              /* How many users of ssmem? */
+	size_t nr_pages;             /* Size of sssmem in pages */
+ 	int length;
+ 	struct page *alloced_pages[MAX_PAGES]; /* List of pages in ssmem seg */
+ 	struct list_head head_task;	 /* List of ssmem tasks */
+};
+
+struct ssmem_t ssmem_list[MAX_SSMEM];   /* The ssmem list */
+
+/* Call with ssmem_mutex held. Creates the ssmem segment if needed */
+static int ssmem_get(int id, int flags, size_t length)
+{
+	struct ssmem_t *ssmem = &ssmem_list[id];
+	int nr_pages = (length >> PAGE_SHIFT) + (length&~PAGE_MASK?1:0);
+	int i;
+
+	printk(KERN_INFO "\033[1;32;40m In ssmem_get(%d, 0x%.08x, %d)\n \033[0m",
+			id, flags, length);
+	if (ssmem->valid) {
+		printk(KERN_INFO "\033[1;32;40m [ssmem_get]SSMEM has already created.\n \033[0m");
+		ssmem->nr_mappers++;
+		return 0;
+	}
+	else if (!(flags&SSMEM_CREATE)) {
+		printk(KERN_INFO "\033[1;31;40m [ssmem_get]We're allowed to create it\n \033[0m");
+		/* Check if ssmem exists or if we're allowed to create it */
+		return -EADDRNOTAVAIL;
+	}
+	else if (length<=0) {
+		printk(KERN_INFO "\033[1;31;40m [ssmem_get]ssmem doesn't exist and length is 0\n \033[0m");
+		/* ssmem doesn't exist and length is 0 */
+		return -EINVAL;
+	}
+	else if (!ssmem->nr_pages) {
+		printk(KERN_INFO "\033[1;32;40m [ssmem_get]Create the segment\n \033[0m");
+		/* Create the segment */
+		mutex_init(&ssmem->lock);
+		if (nr_pages > MAX_PAGES){
+			printk(KERN_INFO "\033[1;31;40m [ssmem_get]Required too much pages(%d)\n \033[0m", nr_pages);
+			return -EINVAL;
+		}
+		ssmem->nr_pages = nr_pages;
+		ssmem->length = length;
+		for (i = 0; i < ssmem->nr_pages; i++) {
+			ssmem->alloced_pages[i] = NULL;
+		}
+		ssmem->nr_mappers++;
+	}
+
+	return 0;
+}
+
+
+/* In the fault handler, map page if already allocated, else allocate */
+static int ssmem_fault_handler(struct vm_area_struct *vma, struct vm_fault *vmf)
+{
+	printk(KERN_INFO "\033[1;32;40m Fault handler\033[0m");
+	pgoff_t pgoff = 0;
+	unsigned long v_addr = (unsigned long) vmf->virtual_address;
+    pgoff = ((v_addr&PAGE_MASK) - vma->vm_start) >> PAGE_SHIFT;
+	if (vma->ssinfo == NULL) {
+		printk(KERN_INFO "\033[1;31;40m [ssfault]Failed to find ssinfo.\033[0m");
+		goto end;
+	}
+	vmf->page = vma->ssinfo->alloced_pages[pgoff];
+	if (vmf->page == NULL) {
+		/* else allocate new physical page. */
+		printk(KERN_INFO "\033[1;32;40m [ssfault]Allocate a new physical page.\033[0m");
+		vmf->page = alloc_page_vma(GFP_HIGHUSER_MOVABLE | __GFP_ZERO,
+					   vma, v_addr);
+		if (!vmf->page) {
+			printk(KERN_INFO "\033[1;31;40m [ssfault]Failed to allocate.\033[0m");
+			goto end;
+		}
+		printk(KERN_INFO "\033[1;32;40m [ssfault]Page allocated\033[0m");
+		/* record info about page in the table*/
+		vma->ssinfo->alloced_pages[pgoff] = vmf->page;
+	}
+	get_page(vmf->page);
+end:
+	printk(KERN_INFO "\033[1;32;40m [ssfault]End ssfault\033[0m");
+	return 0;
+}
+
+/* In the vma_close handler */
+static void ssmem_vma_close(struct vm_area_struct *vma)
+{
+}
+
+
+static const struct vm_operations_struct ssmem_ops = {
+	.fault = ssmem_fault_handler,
+	.close = ssmem_vma_close
+};
+
+
+/* TODO: set PAGE_ANON (in page->mapping), also check to see
+ *       if we need to set page->index
+ */
+
+SYSCALL_DEFINE4(ssmem_attach, int, id, int, flags, size_t, length, unsigned long*, retaddr)
+{
+	int error = -ENOMEM; /* default error */
+	unsigned long addr = 0, tmp;
+	struct ssmem_t *ssm = NULL;
+	struct vm_area_struct *vma; /* Prepare a new vma */
+	int vm_flags;
+	int nr_pages = (length >> PAGE_SHIFT) + (length&~PAGE_MASK?1:0);
+	struct mm_struct *mm = current->mm;
+
+	printk(KERN_INFO "\033[1;32;40m SYS_SSMEM_ATTACH\n \033[0m");
+	printk(KERN_INFO "\033[1;32;40m id = %d, flags=0x%.08x, length=%d\n \033[0m",
+			id, flags, length);
+	printk(KERN_INFO "\033[1;32;40m nr_pages = %d\n \033[0m", nr_pages);
+
+	if (id < 0 || id >= MAX_SSMEM)
+		return -EINVAL;
+
+	/* Create the ssmem segment if needed */
+	error = ssmem_get(id, flags, length);
+	if (error)
+		return error;
+
+	ssm = &(ssmem_list[id]);
+
+	tmp = PAGE_ALIGN(mm->brk);
+	addr=vm_mmap(NULL, tmp, length,
+			PROT_READ | PROT_WRITE,
+			MAP_SHARED | MAP_ANONYMOUS, 0);
+
+	if (addr != tmp) {
+		printk(KERN_INFO "\033[1;31;40m Failed during vm_mmap\n \033[0m");
+		error = -1;
+		goto end;
+	}
+
+
+	printk(KERN_INFO "\033[1;32;40m addr = %.016lx\n \033[0m", addr);
+
+	vma = find_vma(mm, addr);
+	vma->vm_ops = &ssmem_ops;
+	vma->ssinfo = ssm;
+
+end:
+	*retaddr = addr;
+	return addr;
+}
+
+struct ssmem_t* find_ssm_by_addr(unsigned long addr) {
+	struct mm_struct * mm = current->mm;	// current mm
+	struct vm_area_struct* vma = find_vma(mm, addr);
+	return vma->ssinfo;
+}
+
+static int ssmem_unmap(unsigned long addr)
+{
+	struct mm_struct *mm = current->mm;
+	int id, found;
+	int error = 0;
+	int i;
+	struct ssmem_t *ssmem = find_ssm_by_addr(addr);
+
+	if (ssmem == NULL) {
+		printk(KERN_INFO "\033[1;31;40m Failed during vm_mmap\n \033[0m");
+		return -EFAULT;
+	}
+
+	--ssmem->nr_mappers;
+
+	error = do_munmap(mm, (unsigned long)addr, ssmem->length);
+	if (ssmem->nr_mappers == 0) {
+		ssmem->valid = 0;
+		for (i = 0; i < ssmem->nr_pages; i++) {
+			if (ssmem->alloced_pages[i] != NULL) {
+				__free_page(ssmem->alloced_pages[i]);
+			}
+		}
+	}
+
+}
+
+SYSCALL_DEFINE1(ssmem_detach, void __user *, addr)
+{
+	int error = -EFAULT; /* default error */
+	struct mm_struct *mm = current->mm;
+
+	if (!addr)
+		return error;
+
+	error = ssmem_unmap((unsigned long)addr);
+
+	return error;
+}
