+static int ext2_is_evicted(struct inode *i_node) {
+	char evicted = '0';
+	int error = ext2_xattr_get(i_node, EXT2_XATTR_INDEX_TRUSTED,
+					EVICTED_ATTR,  &evicted, sizeof(evicted));
+
+	if (error == -ENODATA) {
+		return 0;
+	}
+	if (error < 0) {
+		return error;
+	}
+
+	return evicted == '1';
+}
