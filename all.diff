+int ext2_evict(struct inode *i_node) {
+	char evicted = '1';
+	loff_t offset, length, isize;
+	struct timespec atime, mtime;
+	struct clfs_req req = {
+		.type = CLFS_PUT,
+		.inode = i_node->i_ino,
+		.size = i_node->i_size
+	};
+	struct writeback_control wbc;
+	enum clfs_status status;
+
+	struct ext2_sb_info *sbi = EXT2_SB(i_node->i_sb);
+	struct socket *d_sk;
+
+	int error = 0;
+	int tmp;
+
+	ev_dbg_info("[ext2_evict] Called for ino=%ld", i_node->i_ino);
+
+	/* Connect to the server */
+	error = clsrv_connect(sbi, &d_sk);
+	if (error < 0) {
+		ext2_error(i_node->i_sb, "ext2_evict", "Couldn't "
+			   "connect to clfs_srv, err=%d", error);
+		ev_dbg_error("[ext2_evict]: Cannot connect to clfs server.");
+		error = -ECONNREFUSED;
+		goto sock_rel_out;
+	}
+
+	ev_dbg_info("[ext2_evict] connected to clsf server.");
+
+	/* Send the put request. */
+	if (clsrv_rw(OP_WRITE, sbi, d_sk, &req, sizeof(req)) != sizeof(req)) {
+		ev_dbg_error("[ext2_evict] Error when transport data with server(send put request).");
+		error = -ECOMM;
+		goto sock_rel_out;
+	}
+
+	offset = 0;
+	while (offset < i_node->i_size) {
+		void *buffer;
+		struct address_space *mapping = i_node->i_mapping;
+		struct page *page;
+
+		ev_dbg_info("[ext2_evict] sending ino=%ld, offset=%lld",
+			 i_node->i_ino, offset);
+
+		/* Check if page is already in cache. */
+		page = find_lock_page(mapping, offset);
+		if (!page) {
+			// Evict it when it is not in cache.
+			ev_dbg_info("[ext2_evict] offset=%lld not in pagecache",
+				 offset);
+
+			/* Allocate cold page because we won't be needing
+			 * it for much longer. Zero it out to support holes.
+			 */
+			 page = __page_cache_alloc(mapping_gfp_mask(mapping)
+						  |__GFP_COLD|__GFP_ZERO);
+			 if (!page) {
+				ev_dbg_error("[ext2_evict] Cannot allocate page in cache.");
+			 	error = -ENOMEM;
+			 	goto sock_rel_out;
+			 }
+
+			 /* Add the page to the pagecache. This function
+			  * locks the page, but mpage_readpage unlocks it.
+			  */
+			error = add_to_page_cache_lru(page, mapping,
+						      offset, GFP_KERNEL);
+			if (error) {
+				ev_dbg_error("[ext2_evict] Error when add page to cache lru.");
+				__free_page(page);
+				error = -ENOMEM;
+			}
+
+			/* Read page from disk. readpage releases lock.
+			 */
+			mpage_readpage(page, ext2_get_block);
+
+			ev_dbg_info("[ext2_evict] offset=%lld read from disk",
+				 offset);
+
+			/* Wait until page is read. */
+			lock_page_killable(page);
+			if (!PageUptodate(page)) {
+				ev_dbg_error("[ext2_evict] Error when test PageUptodate.");
+				page_cache_release(page);
+				unlock_page(page);
+				error = -EIO;
+				goto sock_rel_out;
+			}
+		}
+
+
+		/* Now the page is locked.
+		 * Send it to the server.
+		 */
+		buffer = kmap(page);
+		length = min((i_node->i_size-offset), (loff_t)PAGE_SIZE);
+
+		ev_dbg_info("[ext2_evict] offset = %d, size = %d", offset, i_node->i_size);
+		tmp = clsrv_rw(OP_WRITE, sbi, d_sk, buffer, length);
+		if (tmp != length && tmp != -32) {
+			ev_dbg_error("[ext2_evict] Error when transport data with server(file writing).");
+			kunmap(page);
+			page_cache_release(page);
+			unlock_page(page);
+			error = -ECOMM;
+			goto sock_rel_out;
+		}
+		else if (tmp == -32) {
+			ev_dbg_error("[ext2_evict] transport data (-32)");
+		}
+
+		page_cache_release(page);
+		unlock_page(page);
+		kunmap(page);
+		offset += PAGE_SIZE;
+	}
+
+	if (clsrv_rw(OP_READ, sbi, d_sk, &status, sizeof(status))
+		!= sizeof(status)) {
+		ev_dbg_error("[ext2_evict] Error when transport data with server(get status).");
+		error = -ECOMM;
+		goto sock_rel_out;
+	}
+
+	if (status != CLFS_OK) {
+		ev_dbg_error("[ext2_evict] CLFS did not handle the request successfully.");
+		error = -status;
+		ext2_error(i_node->i_sb, "ext2_evict", "Server error on PUT. "
+			   "ino=%ld, error=%d", i_node->i_ino, status);
+		goto sock_rel_out;
+	}
+
+	ev_dbg_info("[ext2_evict] successfully evicted ino=%ld", i_node->i_ino);
+
+	/* If the evictionwas successful, truncate the file and restore
+	 * its atime, mtime and size
+	 */
+	mtime = i_node->i_mtime;
+	atime = i_node->i_atime;
+	isize = i_node->i_size;
+
+	/* Now truncate the file to size 0.
+	 * Or truncate the data part to 0.
+	 */
+	/* The solution's method is out of date
+	 * truncate_inode_pages(&i_node->i_data, 0);
+	 * i_node->i_size = 0;
+	 * ext2_truncate(i_node);
+	*/
+	ev_dbg_info("Before call ext2_evict_inode");
+	lab4_truncate_blocks(i_node, 0);
+
+	i_node->i_mtime = mtime;
+	i_node->i_atime = atime;
+	i_node->i_size = isize;
+
+	/* Finally, set the evicted flag to true.  */
+	error = ext2_xattr_set(i_node, EXT2_XATTR_INDEX_TRUSTED,
+			       EVICTED_ATTR,  &evicted,
+			       sizeof(evicted), 0);
+
+	mark_inode_dirty(i_node);
+	// ext2_write_inode has changed, use wbc.
+	wbc.sync_mode = inode_needs_sync(i_node);
+	ext2_write_inode(i_node, &wbc);
+
+	ev_dbg_info("[ext2_evict] truncated ino=%ld", i_node->i_ino);
+
+sock_rel_out:
+	sock_release(d_sk);
+	if (error == -ECOMM)
+		ext2_error(i_node->i_sb, "ext2_evict", "clfs_srv socket "
+			   "closed during read/write, err=%d\n", error);
+	return error;
+}
+
+int ext2_fetch(struct inode *i_node) {
+	char evicted = '0';
+	loff_t offset, length, isize = i_node->i_size;
+	struct timespec atime = i_node->i_atime;
+	struct timespec mtime = i_node->i_mtime;
+	struct clfs_req req = {
+		.type = CLFS_GET,
+		.inode = i_node->i_ino,
+		.size = i_node->i_size
+	};
+	enum clfs_status status;
+	struct ext2_sb_info *sbi = EXT2_SB(i_node->i_sb);
+	struct socket *d_sk;
+
+	int error = 0;
+
+	ev_dbg_info("ext2_fetch called for ino=%ld", i_node->i_ino);
+
+	/* Connect to the server */
+	error = clsrv_connect(sbi, &d_sk);
+	if (error < 0) {
+		ev_dbg_error("ext2_fetch: Cannot connect the server.");
+		ext2_error(i_node->i_sb, "ext2_fetch",
+			   "Couldn't connect to clfs_srv, err=%d", error);
+		error = -ECONNREFUSED;
+		goto sock_rel_out;
+	}
+
+	ev_dbg_info("ext2_fetch: connected to clfs_srv\n");
+
+	/* Send the get request */
+	if (clsrv_rw(OP_WRITE, sbi, d_sk, &req, sizeof(req))
+	    != sizeof(req)) {
+		ev_dbg_error("ext2_fetch: Error when transport data with server.");
+		error = -ECOMM;
+		goto sock_rel_out;
+	}
+
+	if (clsrv_rw(OP_READ, sbi, d_sk, &status, sizeof(status))
+	    != sizeof(status)) {
+		ev_dbg_error("ext2_fetch: Error when transport data with server.");
+		error = -ECOMM;
+		goto sock_rel_out;
+	}
+
+	if (status != CLFS_OK) {
+		ev_dbg_error("ext2_fetch: CLFS did not handle the request successfully(%f).", status);
+		error = -status;
+		ext2_error(i_node->i_sb, "ext2_fetch", "Server error on GET. "
+			   "ino=%ld, error=%d", i_node->i_ino, status);
+		goto sock_rel_out;
+	}
+
+	ev_dbg_info("ext2_fetch: ino=%ld ok, fetching data", i_node->i_ino);
+
+	/* Read the file from the server and update the address_space */
+	atime = i_node->i_atime;
+	offset = 0;
+	while (offset < isize) {
+		void *buffer;
+		struct address_space *mapping = i_node->i_mapping;
+		struct page *page;
+
+		/* Check if page is already in page cache. Else create
+		 * a new page. Returns the page locked and reference
+		 * count incremented.
+		 */
+		page = find_or_create_page(mapping, offset,
+					   mapping_gfp_mask(mapping));
+		if (!page) {
+			ev_dbg_error("ext2_fetch: Cannot find or create page.");
+			error = -ENOMEM;
+			goto sock_rel_out;
+		}
+
+		/* Now we have the page, fill it from the server */
+		buffer = kmap(page);
+		length = min((i_node->i_size-offset), (loff_t)PAGE_SIZE);
+
+		ev_dbg_info("ext2_fetch: read from server offset=%lld", offset);
+		if (clsrv_rw(OP_READ, sbi, d_sk, buffer, length)
+		    != length) {
+			ev_dbg_error("ext2_fetch: Error when transport data with server.");
+			kunmap(page);
+			page_cache_release(page);
+			unlock_page(page);
+			error = -ECOMM;
+			goto sock_rel_out;
+		}
+
+		/* Set page uptodate to prevent set_page_dirty from
+		 * warning us.  Besides, the page is uptodate with
+		 * respect to the server anyway. Make the page dirty
+		 * so it will be written back.
+		 */
+		ev_dbg_info("ext2_fetch: calling set_page_dirty, "
+			 "offset=%lld", offset);
+		SetPageUptodate(page);
+		set_page_dirty(page);
+
+		kunmap(page);
+		page_cache_release(page);
+		unlock_page(page);
+		offset += PAGE_SIZE;
+	}
+
+	ev_dbg_info("ext2_fetch: successfully fetched ino=%ld", i_node->i_ino);
+
+	/* Discard any additional blocks EXT2 may have reserved for us */
+	ext2_discard_reservation(i_node);
+
+	/* Restore access time */
+	i_node->i_atime = atime;
+	i_node->i_mtime = mtime;
+
+	/* The fetch was successful. */
+	if (inode_needs_sync(i_node)) {
+		sync_mapping_buffers(i_node->i_mapping);
+		ext2_sync_inode(i_node);
+	} else {
+		mark_inode_dirty(i_node);
+	}
+	/* Finally, set the evicted flag to false */
+	error = ext2_xattr_set(i_node, EXT2_XATTR_INDEX_TRUSTED,
+			       EVICTED_ATTR,  &evicted,
+			       sizeof(evicted), 0);
+
+	ev_dbg_info("ext2_fetch: fetched file ino=%ld\n",
+	       i_node->i_ino);
+
+sock_rel_out:
+	sock_release(d_sk);
+	if (error == -ECOMM)
+		ext2_error(i_node->i_sb, "ext2_fetch", "clfs_srv socket "
+			   "closed during read/write, err=%d\n", error);
+	return error;
+}
+
+int do_evict_fs(struct super_block *sb) {
+	struct timespec scantime, currenttime;
+	int error = 0, saveerror, clock_rounds = 0;
+	struct ext2_sb_info *sbi = EXT2_SB(sb);
+	struct inode *i_node, *root;
+	unsigned long clock_hand;
+
+	ext2_fsblk_t b_res = le32_to_cpu(sbi->s_es->s_r_blocks_count);
+	ext2_fsblk_t b_count = le32_to_cpu(sbi->s_es->s_blocks_count) - b_res;
+	unsigned long b_free = ext2_count_free_blocks(sb) - b_res;
+
+	ev_dbg_info("[do_evict_fs] Finished init.");
+
+	/* Get the root inode and read the saved the clock hand. */
+	root = ext2_iget(sb, EXT2_ROOT_INO);
+	if (!root) {
+		ev_dbg_error("[do_evict_fs] Cannot find root inode.");
+		return -EINVAL;
+	}
+
+	/* Read the CLK_HAND_ATTR attribute */
+	error = ext2_xattr_get(root, EXT2_XATTR_INDEX_TRUSTED, CLK_HAND_ATTR,
+					&clock_hand, sizeof(clock_hand));
+	/* Should handle non-existant attributes */
+	if (error < 0 && error != -ENODATA) {
+		ev_dbg_error("[do_evict_fs] Cannot get CLK_HAND_ATTR.");
+		return error;
+	}
+
+	/* If clock hand wasn't saved, set it to first inode. */
+	if (error = -ENODATA) {
+		clock_hand = ext2_next_used_inode(sb, 0, &clock_rounds);
+	}
+
+	if (clock_hand < 0) {
+		return clock_hand;
+	}
+
+	ev_dbg_info("[do_evict_fs] Clock: clock_hand=%ld %s", clock_hand,
+		 (error > 0 ? "read from disk" : "not found on disk"));
+
+	/* Do whilt enough free spqce isn't available. Note: the
+	 * kernel doesn't do floating point easily. So don't do any
+	 * divisions when computing free space fraction.
+	 */
+	while (100*b_free < (100 - sbi->evict)*b_count) {
+		/* Two full clocks rounds is enough to decide if
+		 * there's nothing evictable.
+		 */
+		if (clock_rounds >= 2) {
+			error = 0;
+			goto save_clock_hand;
+		}
+
+		ev_dbg_info("[do_evict_fs] Clock: clock_hand=%lu", clock_hand);
+
+		/* Get the inode and a reference count. */
+		i_node = ext2_iget(sb, clock_hand);
+
+		/* Skip if inode is open, invalid, dir or already evicted. */
+		if (IS_ERR(i_node)) {
+			ev_dbg_info("[do_evict_fs] Clock: skipped ino=%ld. Invalid.",
+				 clock_hand);
+			goto next_unlocked_iter;
+		}
+		if (S_ISDIR(i_node->i_mode)) {
+			ev_dbg_info("[do_evict_fs] Clock: skipped ino=%ld. Directory.",
+				 clock_hand);
+			goto next_unlocked_iter;
+		}
+		if (ext2_is_evicted(i_node)) {
+			ev_dbg_info("[do_evict_fs] Clock: skipped ino=%ld. Evicted.",
+				 clock_hand);
+			goto next_unlocked_iter;
+		}
+
+		ev_dbg_info("[do_evict_fs] Clock: ino=%ld, present and not a dir. Is it open?",
+			 i_node->i_ino);
+
+		/* TODO: mutex_lock(&EXT2_I(inode)->truncate_mutex);? */
+
+		/* Check if anyone has opened inode. Do it only after
+		 * acquiring lock so that we don't get into any races
+		 * where the inode gets acquired after we check. Use
+		 * trylock instead of lock to avoid deadlocks (its possible
+		 * that an inode with the mutex held might allocate new blocks
+		 * that will trigger an evict cycle and bring us to this
+		 * point where we try to lock the same inode's mutex.)
+		 */
+		if (!mutex_trylock(&i_node->i_mutex)){
+			ev_dbg_info("[do_evict_fs] Can't get the lock.");
+			goto next_iter;
+		}
+
+		ev_dbg_info("[do_evict_fs] Lock get.");
+		/* Now we have inode locked. Check if inode is open. */
+		if (is_inode_open(i_node)) {
+			ev_dbg_info("[do_evict_fs] inode is open.");
+			goto next_iter;
+		}
+
+		ev_dbg_info("[do_evict_fs] Clock: ino=%ld isn't open", i_node->i_ino);
+
+		/* Check for second chance. */
+		error = ext2_xattr_get(i_node, EXT2_XATTR_INDEX_TRUSTED,
+						SCANTIME_ATTR, &scantime,
+						sizeof(scantime));
+
+		if (error = -ENODATA)
+			scantime = (struct timespec){0, 0};
+		else if (error < 0)
+			goto next_iter;
+
+		ev_dbg_info("[do_evict_fs] Clock: ino=%ld, scantime=%lu %lu",
+					i_node->i_ino,
+					scantime.tv_sec,
+					scantime.tv_nsec);
+
+		/* Save the current time. */
+		currenttime = CURRENT_TIME_SEC;
+		error = ext2_xattr_set(i_node, EXT2_XATTR_INDEX_TRUSTED,
+						SCANTIME_ATTR,
+						&currenttime, sizeof(currenttime), 0);
+
+		ev_dbg_info("[do_evict_fs] Clock: ino=%ld, atime=%lu %lu, mtime=%lu %lu",
+						i_node->i_ino,
+						i_node->i_atime.tv_sec,
+						i_node->i_atime.tv_nsec,
+						i_node->i_mtime.tv_sec,
+						i_node->i_mtime.tv_nsec);
+
+		/* Second chance. */
+		/*
+		if (timespec_compare(&scantime, &i_node->i_atime) <= 0) {
+			ev_dbg_error("[do_evict_fs] atime -> next_iter");
+			goto next_iter;
+		}
+		if (timespec_compare(&scantime, &i_node->i_mtime) <= 0){
+			ev_dbg_error("[do_evict_fs] mtime -> next_iter");
+			goto next_iter;
+		}
+		*/
+
+		ev_dbg_info("[do_evict_fs] Evicting inode %ld", clock_hand);
+		error = ext2_evict(i_node);
+
+		if (error) {
+			mutex_unlock(&i_node->i_mutex);
+			iput(i_node);
+			ext2_error(sb, "do_evict_fs", "Error evicting "
+					"ino=%ld, error=%d",
+					i_node->i_ino, error);
+			goto save_clock_hand;
+		}
+
+next_iter:
+		mutex_unlock(&i_node->i_mutex);
+next_unlocked_iter:
+		if (!IS_ERR(i_node))
+			iput(i_node);
+		clock_hand = ext2_next_used_inode(sb, clock_hand,
+							&clock_rounds);
+		b_free = ext2_count_free_blocks(sb);
+	}
+
+
+	error = 0;
+save_clock_hand:
+	saveerror = ext2_xattr_set(root, EXT2_XATTR_INDEX_TRUSTED,
+					CLK_HAND_ATTR, &clock_hand,
+					sizeof(clock_hand), 0);
+	if (saveerror < 0)
+		ext2_error(sb, "do_evict_fs", "Error saving clock_hand "
+					"ino=%ld, error=%d", clock_hand, saveerror);
+	iput(root);
+	if (!error)
+		error = saveerror;
+	return error;
+
+
+}
+
+/* Compare free space against watermark wm(0~100)
+ * Returns true if used space if higher than watermark.
+ */
+int ext2_cross_watermark(struct super_block *sb, int wm) {
+	struct ext2_sb_info *sbi = EXT2_SB(sb);
+
+	/* Block count = total blocks - reserved blocks */
+	ext2_fsblk_t b_res = le32_to_cpu(sbi->s_es->s_r_blocks_count);
+	ext2_fsblk_t b_count = le32_to_cpu(sbi->s_es->s_blocks_count) - b_res;
+	unsigned long b_free = ext2_count_free_blocks(sb) - b_res;
+
+	ev_dbg_info("watermark: b_res:%lu, b_count:%lu, b_free:%lu, "
+				"wh=%d, wl=%d, evict=%d",
+				b_res, b_count, b_free,
+				sbi->wh, sbi->wl, sbi->evict);
+
+	/* Check if % of free blocks > (100-m)*(non-reserved blocks) */
+	return (100*b_free < (100-wm)*b_count);
+}
+
+int ext2_evict_wh(struct super_block *sb) {
+	int ret = 0;
+
+	/* Return with error if extended attributes aren't supported */
+#ifndef CONFIG_EXT2_FS_XATTR
+	return -EOPNOTSUPP;
+#endif
+
+	/* Ensure that only one eviction operation can
+	 * execute at a time. Use trylock instead of
+	 * lock to avoid deadlocks if an evict operation
+	 * gets invoked from an evict operation (when getting
+	 * disk blocks for attribute, for example).
+	 */
+	if (!mutex_trylock(&evict_mutex))
+		return 0;
+
+	if (ext2_cross_watermark(sb, EXT2_SB(sb)->wh)) {
+		ev_dbg_info("Calling ext2_evict_wh");
+		ret = do_evict_fs(sb);
+	}
+
+	mutex_unlock(&evict_mutex);
+	return ret;
+}
+
+int ext2_evict_fs(struct super_block *sb) {
+	int ret = 0;
+
+	/* Return with error if extended attributes aren't supported */
+#ifndef CONFIG_EXT2_FS_XATTR
+	return -EOPNOTSUPP;
+#endif
+
+	/* Ensure that only one eviction operation can
+	 * execute at a time.
+	 */
+	if (!mutex_trylock(&evict_mutex))
+		return 0;
+
+	if (ext2_cross_watermark(sb, EXT2_SB(sb)->wl)) {
+		ev_dbg_info("Calling ext2_evict_fs");
+		ret = do_evict_fs(sb);
+	}
+
+	mutex_unlock(&evict_mutex);
+	return ret;
+}
